<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TopBar">
        <attr name="topbarTitle" format="string" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="titleTextColor" format="color" />
        <attr name="leftVisible" format="boolean" />
        <attr name="rightIcon" format="reference" />
        <attr name="rightVisible" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SettingItem">
        <attr name="name" format="string" />
        <attr name="rightText" format="string" />
        <attr name="settingRightVisible" format="boolean" />
        <attr name="isGroupFirst" format="boolean" />
        <attr name="isGroupLast" format="boolean" />
    </declare-styleable>
    <declare-styleable name="EntryItem">
        <attr name="entryTitle" format="string" />
        <attr name="imgSrc" format="reference" />
    </declare-styleable>
    <declare-styleable name="ArticleListItem">
        <attr name="articleTitle" format="string" />
        <attr name="rank" format="string" />
    </declare-styleable>
</resources>